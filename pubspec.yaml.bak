name: dialysis_tracker
description: "An easy to use tracker for dialysis patients."
publish_to: 'none'
version: 1.0.0+1

environment:
  sdk: ^3.9.0

dependencies:
  flutter:
    sdk: flutter
  cupertino_icons: ^1.0.8
  sqflite: ^2.3.3
  path_provider: ^2.1.4
  shared_preferences: ^2.2.3
  image_picker: ^1.1.2
  permission_handler: ^11.3.1
  camera: ^0.11.0
  table_calendar: ^3.1.2
  intl: ^0.19.0

dev_dependencies:
  flutter_test:
    sdk: flutter
  flutter_lints: ^5.0.0
  flutter_launcher_icons: ^0.13.1   # (or ^0.14.4)

flutter_icons:
  android: true
  ios: true
  image_path: "assets/icon/icon_foreground.png"     # fallback for non-adaptive
  adaptive_icon_foreground: "assets/icon/icon_foreground.png"
  adaptive_icon_background: "assets/icon/icon_background.png"

flutter:
  uses-material-design: true

